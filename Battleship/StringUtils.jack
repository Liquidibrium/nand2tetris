class StringUtils {

    function void toString(int i){
        var String tmp; 
        let tmp = String.new(20);
        do tmp.setInt(i);
        do Output.printString(tmp);
        do tmp.dispose();
        return;
    }

    function void to(int i, int j){
        do StringUtils.toString(i);
        do Output.printString(" ");
        do StringUtils.toString(j);
        do Output.println();
        return;
    }

}